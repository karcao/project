<view>
    <view class="login-icon">
        <image class="login-img" src="../../images/swiper.jpg"></image>
    </view>
    <view class="login-from">
        <form bindsubmit='doRegist'>
            <view class="inputView">
                <view class='inputleft'>
                    <image class="nameImage" src="../../images/icon/compay.png"></image>
                    <label class="loginLabel">公司名称</label>
                </view>
                <view>
                    <view class="inputText1">{{companyname}}</view>
                    <!-- <input name="comname" value='{{companyname}}' class="inputText" placeholder="请输入公司名称" /> -->
                </view>
            </view>
            <view class="line"></view>
            <!--账号-->
            <view class="inputView">
                <view class='inputleft'>
                    <image class="nameImage" src="../../images/icon/username.png"></image>
                    <label class="loginLabel">账号</label>
                </view>
                <view>
                    <input name="username" class="inputText" placeholder="请输入账号" />
                </view>
            </view>
            <view class="line"></view>
            <!--密码-->
            <view class="inputView">
                <view class='inputleft'>
                    <image class="keyImage" src="../../images/icon/password.png"></image>
                    <label class="loginLabel">密码</label>
                </view>
                <view>
                    <input name="password" class="inputText" password="true" placeholder="请输入密码" />
                </view>
            </view>
            <view class="line"></view>
            <!--密码-->
            <view class="inputView">
                <view class='inputleft'>
                    <image class="keyImage" src="../../images/icon/password.png"></image>
                    <label class="loginLabel">确认密码</label>
                </view>
                <view>
                    <input name="password1" class="inputText" password="true" placeholder="请确认密码" />
                </view>
            </view>
            <!--按钮-->
            <view>
                <button class="loginBtn" type="primary" form-type='submit'>注册</button>
            </view>

            <view>
                <button class="goLoginBtn" type="warn" bindtap="goLoginPage">返回登录</button>
            </view>
        </form>
    </view>
</view>